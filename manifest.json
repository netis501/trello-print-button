{
  "manifest_version": 1,
  "name": "Trello Print Power-Up",
  "description": "A Power-Up to add print functionality to Trello.",
  "icon": {
    "url": "https://netis501.github.io/trello-print-button/icon.png"
  },
  "author": "Your Name",
  "capabilities": [
    "board-buttons"
  ],
  "board-buttons": [
    {
      "text": "Print",
      "condition": "always",
      "icon": {
        "url": "https://netis501.github.io/trello-print-button/print-icon.png",
        "size": "small"
      },
      "url": "https://netis501.github.io/trello-print-button/index.html",
      "target": "InBoard"
    }
  ]
}
